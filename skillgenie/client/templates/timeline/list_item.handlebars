
<div class="item-headline"><h5>{{ headline }}</h5></div>
<p>Created on: {{ timestamp }}</p>


<div class="item-buttons">
        <a href="#timeline/{{ _id }}" class="btn btn-small js-show">
          <i class="small mdi-av-web"></i>
        </a>
        <a href="#timeline/{{ _id }}/edit" class="btn btn-small js-edit">
           <i class="small mdi-editor-mode-edit"></i>
        </a>
        <button class="btn btn-small js-delete">
          <i class="small mdi-content-content-cut"></i>
        </button> 
</div>